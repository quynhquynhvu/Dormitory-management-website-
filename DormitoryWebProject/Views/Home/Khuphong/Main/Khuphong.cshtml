@{
    ViewBag.Title = "Khu phòng";
}

@section PageStyle{
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link href="~/assets/css/khuphong/Khu-Phong-So-do.css" rel="stylesheet" />
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2" style="margin-top:1em;">
                <div class="col-sm-6">
                    <h1>SƠ ĐỒ PHÒNG</h1>
                    <br />
                </div>

                <div class="col-sm-6">
                    <div class=" float-sm-right">
                        <!-- Button trigger modal -->
                        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal" style="width: 13em;">
                            <i class="fas fa-search"></i>
                            Tìm kiếm nâng cao
                        </button>
                    </div>
                </div>
            </div>

            <!-- Modal -->
            <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="ModalLabel">Tìm kiếm Khu-Phòng</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <table style="width:100%">
                                <tr>
                                    <th>Kí túc xá</th>
                                    <td>
                                        <select class="form-control">
                                            <option>Chọn KTX</option>
                                            <option>KTX 135A Trần Hưng Đạo</option>
                                            <option>KTX 43-45 Nguyễn Chí Thanh</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <th>Tầng</th>
                                    <td>
                                        <select class="form-control">
                                            <option>Chọn Tầng</option>
                                            <option name="tang">Tầng 1</option>
                                            <option name="tang">Tầng 2</option>
                                            <option name="tang">Tầng 3</option>
                                            <option name="tang">Tầng 4</option>
                                            <option name="tang">Tầng 5</option>
                                            <option name="tang">Tầng 6</option>
                                            <option name="tang">Tầng 7</option>
                                            <option name="tang">Tầng 8</option>
                                            <option name="tang">Tầng 9</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <th>Loại phòng</th>
                                    <td>
                                        <select class="form-control">
                                            <option>Chọn loại phòng</option>
                                            <option>Phòng Chất lượng cao</option>
                                            <option>Phòng Thường</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <th>Trạng thái phòng</th>
                                    <td>
                                        <label class="container" style="font-weight: normal;">
                                            <input type="radio" checked="checked" name="radio">
                                            <span class="checkmark">Tất cả</span>
                                        </label>
                                        <label class="container" style="font-weight: normal;">
                                            <input type="radio" name="radio">
                                            <span class="checkmark">Còn trống</span>
                                        </label>
                                        <label class="container" style="font-weight: normal;">
                                            <input type="radio" name="radio">
                                            <span class="checkmark">Hết chỗ</span>
                                        </label>
                                    </td>
                                </tr>
                            </table>

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Đóng</button>
                            <button type="button" class="btn btn-primary">Tìm kiếm</button>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <br />
    </section>

